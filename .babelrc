{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    [
      "transform-inline-environment-variables",
      {
        "include": [
          "LOG_LEVEL"
        ]
      }
    ]
  ]
}